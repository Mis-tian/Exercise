{
    "presets":["react","es2015","stage-0"],
    "env":{
        "development":{
            "presets":["react-hmre"]
        }
    },
    "plugins": [
        [
            "import", 
            [
                {
                    "libraryName": "antd",
                    "libraryDirectory": "lib",   // default: lib 
                    "style": true
                },
                {
                    "libraryName": "antd-mobile",
                    "libraryDirectory": "component"
                }
            ],
            [
              "transform-runtime",
                {
                    "polyfill": false,
                    "regenerator": true
                }
            ] 
        ]
    ]
}